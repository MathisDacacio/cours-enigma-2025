{
  "name": "enigma",
  "version": "1.0.0",
  "description": "Une application d'exemple pour lire la liste des espèces menacées dans un pays triées par année",
  "main": "src/index.js",
  "module": "src/index.js",
  "exports": [],
  "scripts": {
    "dev": "node src/app.js",
    "debug": "nodemon --inspect src/app.js",
    "prettier": "npx prettier . --write",
    "prepare": "husky",
    "linter": "npx eslint",
    "test": "jest --verbose --passWithNoTests"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "files": [
    "src/**"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Riduidel/cours-enigma-2025.git"
  },
  "keywords": [
    "tutorial",
    "vitest",
    "express.js",
    "iucn"
  ],
  "author": "Nicolas Delsaux",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Riduidel/cours-enigma-2025/issues"
  },
  "homepage": "https://github.com/Riduidel/cours-enigma-2025#readme",
  "dependencies": {
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "prettier": "3.4.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
